<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE config SYSTEM "./core_config.dtd">
<config>
	<accounts>
		<list>
			<account
				id="1"
				name="NAMEx 1"
				username="USERNAME #1"
				domain="DOMAIN #1"
				authusername="AUTHUSERNAME #1"
				password="PASSWORD #1"
				displayname="DISPLAYNAME #1"
				outboundproxy="OUTBOUNDPROXY #1"
				registrar="PASSWORD #1"
				autoregister="false"
			/>
			<account
				id="2"
				name="NAMEy #2"
				username="USERNAME #2"
				domain="DOMAIN #2"
				authusername="AUTHUSERNAME #2"
				password="PASSWORD #2"
				displayname="DISPLAYNAME #2"
				outboundproxy="OUTBOUNDPROXY #2"
				registrar="PASSWORD #2"
				autoregister="false"
			/>
			<account
				id="3"
				name="NAME #3"
				username="USERNAME #3"
				domain="DOMAIN #3"
				authusername="AUTHUSERNAME #3"
				password="PASSWORD #3"
				displayname="DISPLAYNAME #3"
				outboundproxy="OUTBOUNDPROXY #3"
				registrar="PASSWORD #3"
				autoregister="true"
			/>
			<account
				id="4"
				name="NAME 4"
				username="USERNAME #4"
				domain="DOMAIN #4"
				authusername="AUTHUSERNAME #4"
				password="PASSWORD #4"
				displayname="DISPLAYNAME #4"
				outboundproxy="OUTBOUNDPROXY #4"
				registrar="PASSWORD #4"
				autoregister="true"
			/>
			<account
				id="5"
				name="NAME 5"
				username="USERNAME #5"
				domain="DOMAIN #5"
				authusername="AUTHUSERNAME #5"
				password="PASSWORD #5"
				displayname="DISPLAYNAME #5"
				outboundproxy="OUTBOUNDPROXY #5"
				registrar="PASSWORD #5"
				autoregister="false"
			/>
			<account
				id="6"
				name="NAMEz 6"
				username="USERNAME #6"
				domain="DOMAIN #6"
				authusername="AUTHUSERNAME #6"
				password="PASSWORD #6"
				displayname="DISPLAYNAME #6"
				outboundproxy="OUTBOUNDPROXY #6"
				registrar="PASSWORD #6"
				autoregister="false"
			/>
		</list>
	</accounts>
	<core>
		<events>
			<dispatcher
				maxcalls="32"
				maxevents="16"
			/>
		</events>
	</core>
	<remote>
		<callback>
			<guicallback
				guiurl="http://gungnir:8888/RPC2"
			/>
			<xmlrpcclient
				name="Core CallbackClient"
				version="1.0"
			/>
		</callback>
		<server>
			<xmlrpcserver
				port="7777"
				registerprefix="core"
				logfilename="/tmp/xmlrpc_log"
			/>
		</server>
	</remote>
	<sipstack>
		<exosipadapter
			port="5065"
		/>
	</sipstack>
	<util>
		<logging>
			<simplelogger>
				<console
					enabled="true"
					loglevel="LOG_MESSAGE"   
				/>
				<file
					enabled="true"
					filename="/tmp/sipoks05core.log"
					loglevel="LOG_DEBUG"
					withtime="true"
					verbose="true"
					threadid="true"
				/>
			</simplelogger>
		</logging>
		<threadmanagement>
			<threads
				maxamount="64"
			/>
		</threadmanagement>
	</util>
</config>
